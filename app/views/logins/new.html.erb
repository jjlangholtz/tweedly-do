<div class='columns large-4 large-centered'>
  <%= form_tag logins_path do %>
    <%= label_tag :user_id, 'Name' %>
    <%= select_tag :user_id,
          options_for_select(@users.map { |user| [user.name, user.id] }) %>
    <%= submit_tag 'Login', class: 'button small success' %>
    <%= link_to 'New User?', new_user_path, class: 'button small right' %>
  <% end %>
</div>
